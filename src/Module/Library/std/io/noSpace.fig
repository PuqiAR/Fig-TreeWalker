import _builtins;

public func print(objects...) -> Int
{
    var length := objects.length();
    for var i:=0; i < length; i += 1
    {
        __fstdout_print(objects[i]);
    }
    return length;
}

public func println(objects...) -> Int
{
    var length := objects.length();
    for var i:=0; i < length; i += 1
    {
        __fstdout_print(objects[i]);
    }
    __fstdout_print("\n");
    return length + 1;
}